{
  "kind": "collectionType",
  "collectionName": "appbooks",
  "info": {
    "singularName": "appbook",
    "pluralName": "appbooks",
    "displayName": "AppBook",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 256,
      "minLength": 1
    },
    "private": {
      "type": "boolean",
      "required": true
    },
    "imageUrl": {
      "type": "string",
      "required": true,
      "minLength": 1
    },
    "websiteUrl": {
      "type": "string",
      "required": true,
      "minLength": 1
    },
    "color": {
      "type": "string",
      "required": true,
      "minLength": 1,
      "unique": false
    },
    "popular": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "apptopic": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::apptopic.apptopic",
      "inversedBy": "appbooks"
    },
    "description": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "language": {
      "type": "string",
      "required": true,
      "default": "en",
      "minLength": 1,
      "maxLength": 256
    }
  }
}
