{
  "kind": "collectionType",
  "collectionName": "apparticles",
  "info": {
    "singularName": "apparticle",
    "pluralName": "apparticles",
    "displayName": "AppArticle",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
